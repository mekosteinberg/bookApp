<%- include('./partials/header') %>

    <div>
        <a href="/new">
            <button>Add a Book</button>
        </a>
        <% data.forEach((book)=> {%>
            <div class="container">
                <div class="row">
                  <a href="/bookdetails">
                    <h6 class="col">
                        <%=book.title%>
                    </h6>
                    <h6 class="col">
                        <%=book.authorLast%>, <%=book.authorFirst%>
                    </h6>
                </a>
                </div>
            </div>
            <%})%>
    </div>

    <%- include('./partials/footer') %>